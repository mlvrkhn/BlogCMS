{
	"extends": ["airbnb", "airbnb/hooks"],
	"rules": {
		"react/jsx-filename-extension": [0],
		"indent": ["warn", 4],
		"react/jsx-indent": ["warn", 4],
		"react/jsx-indent-props": ["warn", 4],
		"react/no-array-index-key": [0],
		"no-tabs": [0],
	},
	"globals": {
		"window": true,
		"document": true
	}
}
